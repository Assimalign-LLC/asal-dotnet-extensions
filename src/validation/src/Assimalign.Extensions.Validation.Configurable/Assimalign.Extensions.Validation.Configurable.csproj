<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<PackageTags>validation;configurable</PackageTags>
		<PackageId>Assimalign.Extensions.Validation.Configurable</PackageId>
		<AssemblyName>Assimalign.Extensions.Validation.Configurable</AssemblyName>
		<RootNamespace>Assimalign.Extensions.Validation.Configurable</RootNamespace>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<Description>
			A library 
		</Description>

		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageIcon>.assimalign.logo.png</PackageIcon>
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\Assimalign.Extensions.Validation\Assimalign.Extensions.Validation.csproj" />
	</ItemGroup>
	<ItemGroup>
		<None Include="..\..\..\..\.assimalign.logo.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>
	<ItemGroup>
		<None Include="README.md">
			<Pack>true</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>
</Project>
