<Project>
	<PropertyGroup>
		<Version>1.0.0-pre.1.0.2</Version>
		<TargetFrameworks>net5.0;net6.0</TargetFrameworks>
		<GeneratePackageOnBuild Condition="$(RootNamespace) != 'Assimalign.Extensions.Scheduling' and $(RootNamespace) != 'Assimalign.Extensions.Diagnostics'">true</GeneratePackageOnBuild>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<Authors>Chase Crawford</Authors>
		<Copyright>Â© Assimalign LLC 2021</Copyright>
		<PackageIcon>.assimalign.logo.png</PackageIcon>
		<PackageReadmeFile>readme.md</PackageReadmeFile>
		<PackageIconUrl>https://corporate.assets.assimalign.com/assets/logos/.assimalign.logo.png</PackageIconUrl>
		<RepositoryUrl>https://github.com/Assimalign-LLC/asal-dotnet-extensions</RepositoryUrl>
	</PropertyGroup>
	<ItemGroup>
		<None Include="..\..\.assimalign.logo.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>
</Project>